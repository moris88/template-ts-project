{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint","prefer-arrow"],
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"]
    }
}
